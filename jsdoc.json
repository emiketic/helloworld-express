{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./app"],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/_jsdoc",
    "recurse": true,
    "private": true,
    "lenient": true
  }
}
